{
	"compilerOptions": {
		"target": "es2020",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noEmit": true,
		"checkJs": true,
		"isolatedModules": true,

		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	}
}
