{
	"name": "mauss",
	"version": "0.9.0",
	"description": "lightweight, modular, type-safe utilities",
	"repository": "github:alkamauss/mauss",
	"author": "Ignatius Bagus",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"check": "pnpm build:files",
		"test": "uvu -r tsm src \"(spec\\.ts)\"",
		"build:types": "tsm scripts/compile-types.ts",
		"build:files": "tsc --project src",
		"prepublishOnly": "pnpm run \"/^build:.*/\""
	},
	"typings": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./src/core/index.js"
		},
		"./api": {
			"types": "./index.d.ts",
			"default": "./src/api/index.js"
		},
		"./bits": {
			"types": "./index.d.ts",
			"default": "./src/bits/index.js"
		},
		"./compare": {
			"types": "./index.d.ts",
			"default": "./src/compare/index.js"
		},
		"./csv": {
			"types": "./index.d.ts",
			"default": "./src/csv/index.js"
		},
		"./date": {
			"types": "./index.d.ts",
			"default": "./src/date/index.js"
		},
		"./guards": {
			"types": "./index.d.ts",
			"default": "./src/guards/index.js"
		},
		"./math": {
			"types": "./index.d.ts",
			"default": "./src/math/index.js"
		},
		"./promises": {
			"types": "./index.d.ts",
			"default": "./src/promises/index.js"
		},
		"./random": {
			"types": "./index.d.ts",
			"default": "./src/random/index.js"
		},
		"./std": {
			"types": "./index.d.ts",
			"default": "./src/std/index.js"
		},
		"./sys": {
			"types": "./index.d.ts",
			"default": "./src/sys/index.js"
		},
		"./web": {
			"types": "./index.d.ts",
			"default": "./src/web/index.js"
		},
		"./typings": {
			"types": "./index.d.ts"
		},
		"./package.json": "./package.json",
		"./prettier.config.js": "./prettier.config.js",
		"./tsconfig.json": "./tsconfig.json"
	},
	"files": [
		"src/**/*.js",
		"index.d.ts",
		"index.d.ts.map",
		"prettier.config.js",
		"tsconfig.json"
	],
	"keywords": [
		"lightweight",
		"modular",
		"type-safe",
		"utility",
		"zero-dependency",
		"tree-shakable",
		"strongly-typed",
		"efficient",
		"functional",
		"declarative",
		"opinionated",
		"settings"
	]
}
