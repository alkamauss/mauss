{
	"name": "mauss",
	"version": "0.9.2",
	"description": "the essential utility belt â€” modular, type-safe, and dependency-free",
	"repository": "github:alkamauss/mauss",
	"author": "Ignatius Bagus",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"check": "pnpm build:files",
		"test": "pnpm run \"/^test:.*/\"",
		"test:unit": "uvu -r tsm src \"(spec\\.ts)\"",
		"test:types": "tsc -p src/tsconfig.test.json",
		"build:types": "tsm scripts/compile-types.ts",
		"build:files": "tsc --project src",
		"prepublishOnly": "pnpm run \"/^build:.*/\""
	},
	"typings": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./src/core/index.js"
		},
		"./api": {
			"types": "./index.d.ts",
			"default": "./src/api/index.js"
		},
		"./bits": {
			"types": "./index.d.ts",
			"default": "./src/bits/index.js"
		},
		"./compare": {
			"types": "./index.d.ts",
			"default": "./src/compare/index.js"
		},
		"./csv": {
			"types": "./index.d.ts",
			"default": "./src/csv/index.js"
		},
		"./guards": {
			"types": "./index.d.ts",
			"default": "./src/guards/index.js"
		},
		"./math": {
			"types": "./index.d.ts",
			"default": "./src/math/index.js"
		},
		"./promises": {
			"types": "./index.d.ts",
			"default": "./src/promises/index.js"
		},
		"./random": {
			"types": "./index.d.ts",
			"default": "./src/random/index.js"
		},
		"./std": {
			"types": "./index.d.ts",
			"default": "./src/std/index.js"
		},
		"./web": {
			"types": "./index.d.ts",
			"default": "./src/web/index.js"
		},
		"./typings": {
			"types": "./index.d.ts"
		},
		"./package.json": "./package.json",
		"./prettier.config.js": "./prettier.config.js",
		"./tsconfig.json": "./tsconfig.json"
	},
	"files": [
		"src/**/*.js",
		"index.d.ts",
		"index.d.ts.map",
		"prettier.config.js",
		"tsconfig.json"
	],
	"keywords": [
		"config",
		"utility",
		"modular",
		"type-safe",
		"functional",
		"lightweight",
		"tree-shakable",
		"zero-dependencies",
		"declarative",
		"typescript",
		"opinionated"
	]
}
